URxvt*termName:rxvt-unicode-256color

Xft.antialias:  true
Xft.autohint:   false
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcdlight
Xft.rgba:       rgb
Xft.dpi:        96

!Fonts
!URxvt.font: xft:Inconsolata:size=11
URxvt.font: xft:Monaco:size=10
URxvt.letterSpace: -1

URxvt*transparent: true
URxvt*.shading: 20

! --- ~/.Xresources ------------------------------------------------------------
! ------------------------------------------------------------------------------
! --- generated with 4bit Terminal Color Scheme Designer -----------------------
! ------------------------------------------------------------------------------
! --- http://ciembor.github.com/4bit -------------------------------------------
! ------------------------------------------------------------------------------

! --- special colors ---

*background: #0d1926
*foreground: #d9e6f2

! --- standard colors ---

! black
*color0: #000000

! bright_black
*color8: #262626

! red
*color1: #c6726c

! bright_red
*color9: #e3b8b5

! green
*color2: #6cc672

! bright_green
*color10: #b5e3b8

! yellow
*color3: #c0c66c

! bright_yellow
*color11: #e0e3b5

! blue
*color4: #726cc6

! bright_blue
*color12: #b8b5e3

! magenta
*color5: #c66cc0

! bright_magenta
*color13: #e3b5e0

! cyan
*color6: #6cc0c6

! bright_cyan
*color14: #b5e0e3

! white
*color7: #d9d9d9

! bright_white
*color15: #ffffff


! ------------------------------------------------------------------------------
! --- end of terminal colors section -------------------------------------------
! ------------------------------------------------------------------------------


!Say no to scrollbars
URxvt*scrollBar:false


URxvt.imLocale:en_GB.UTF8

URxvt*skipBuiltinGlyphs: false
!
! Amount that I can scroll up
URxvt.saveLines: 12000

! Underline URLs
URxvt.underlineURLs:true

! Clickable urls
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

URxvt.iso14755_52:false
URxvt.iso14755:false

! Add color to my!man pages:
URxvt.colorIT: #87af5f
URxvt.colorBD: #d7dfdf
!URxvt.colorUL:


